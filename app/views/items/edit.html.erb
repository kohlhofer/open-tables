<% @page_title = 'Edit item' %>

<%= error_messages_for :item %>

<% form_for(@item, :html => { :multipart => true }) do |f| %>
  <%= render :partial => 'form' , :locals => {:f => f} %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @item %> |
<%= link_to 'Back', items_path %>
